    "DB_CLIENT_CERT": "@db-client-cert",
    "DB_CLIENT_KEY": "@db-client-key",
    "DB_SERVER_CA": "@db-server-ca",
    "ETHERSCAN_API_KEY": "@eth-api-key",
    "ETHERSCAN_DB_URL": "@eth-db-url",
