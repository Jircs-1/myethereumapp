{
  "extends": [
    "airbnb",
    "plugin:jest/recommended"
  ],
  "globals": {
    "__CLIENT_CSS__": false,
    "__CLIENT_JS__": false,
    "__GIT_SHA__": false
  },
  "plugins": [
    "jest"
  ],
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "import/prefer-default-export": 0,
    "no-underscore-dangle": 0,
    "react/jsx-filename-extension": 0,
    "react/forbid-prop-types": 0
  }
}
